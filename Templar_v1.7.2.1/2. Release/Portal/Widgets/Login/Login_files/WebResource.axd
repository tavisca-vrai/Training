.MyWidget { overflow: hidden; background-color: white; border: solid 1px #CCCCCC; padding: 0px; }
.NonDrag-MyWidget { overflow: hidden; background-color: white; border: solid 1px #CCCCCC; padding: 0px; }
.MyHeader { cursor: move; line-height: 12px; }
    .MyHeader a { color: White; }
.widget_hover { border: solid 1px #7B7B7B; padding: 0px; }
.placeholder { border: dashed 2px gray; margin-bottom: 20px; min-height: 30px; }

.HeaderPanel, .FooterPanel { min-height: 50px; }
.LeftPanel, .MiddlePanel, .RightPanel { float: left; min-height: 250px; }
.HeaderPanel, .LeftPanel, .MiddlePanel, .RightPanel, .FooterPanel { border: 1px solid #0066FF; }


