/*---- Event Map---*/
.ContentHeader { padding: 3px; font-size: 11px; background-color: rgb(238, 238, 238); color: rgb(0, 0, 0); text-align: center; border: 1px solid rgb(0, 0, 0); vertical-align: top; text-transform: uppercase; }
.EventInfo fieldset { margin: 10px; padding: 5px; }
.EventInfo .UnMapped { background-color: #5C5858; border: 1px solid green; color: white; cursor: pointer; }
.EventInfo .EventName { border: 1px solid #D8DFEA; height: 20px; padding: 2px; cursor: pointer; }
.EventInfo .SelectedSource { background-color: #8A00FF; font-weight: bold; color: White; }
.EventInfo .AttachedTarget { background-color: #7CCFAD; border: 1px solid green; font-weight: bold; }

